package Ejercicios;
import java.util.Scanner;

public class Ejercicio_26{
	public static void main(String[]Args) {
		Scanner entrada = new Scanner (System.in);
		int num,ac=0,media;
		System.out.println("Introduzca los números a realizar la media");
		num=entrada.nextInt();
		for (int i=0;i<num;i++) {
			num=entrada.nextInt();
			ac=ac+num;
			media = ac/num;
			
		}
	}
}